<div id="channel_{{c_id}}">
    <div class="progress">
        <div class="progress-bar" role="progressbar" aria-valuenow="{{percent}}" aria-valuemin="0" aria-valuemax="100" style="width:{{percent}}%">
        </div>
        <div class="progress-bar-title left-title">{{c_id}} {{alias}} - {{percent}}% {{weather}}</div>
        <div class="progress-bar-title right-title">{{cur}}/{{goal}}</div>
    </div>
</div>